\ProvidesPackage{pictikzgraph}[2017/09/03 v1.1]

\tikzset{
	pictikz-node/.style={
		inner sep=1.0pt,
		circle,
		draw,
		radius=3pt,
		minimum size=6pt
	},
	pictikz-rectangle/.style={
		rectangle,
		draw,
		inner sep=3pt,
		minimum width=2cm
	},
	pictikz-edgeto/.style={
		->,
		shorten <=2pt,
		shorten >=2pt,
	},
	pictikz-edgeboth/.style={
		<->,
		shorten <=2pt,
		shorten >=2pt,
	},
	pictikz-edgefrom/.style={
		<-,
		shorten <=2pt,
		shorten >=2pt,
	},
	pictikz-thick/.style={
		very thick
	},
	pictikz-dashed/.style={
		thick,
		dashed,
	},
	pictikz-dotted/.style={
		thick,
		dotted,
	}
}
\definecolor{pictikz-black}{RGB}{0, 0, 0}
\definecolor{pictikz-white}{RGB}{255, 255, 255}
